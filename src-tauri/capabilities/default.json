{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:default",
    "dialog:default",
    "fs:default",
    "fs:scope-desktop",
    "fs:scope-document",
    "fs:write-all",
    "fs:read-all",
    "fs:allow-exists",
    "dialog:default",
    "shell:allow-open",
    "shell:allow-execute"
  ]
}